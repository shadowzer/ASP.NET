<h4> Hardcoded username&password: user - password</h4>
<div>
    Username
    <input type="text" name="username" id="username" />
</div>
<div>
    Password
    <input type="text" name="password" id="password" />
</div>
<div>
    <a href="javascript:submit();">Log in link</a>
</div>
<div>
    <input type="button" value="Log in button" onclick="submit()"/>
</div>
<div>
    <a href="https://oauth.vk.com/authorize?client_id=5757007&display=popup&redirect_uri=http://localhost:1239/Home/Contact&scope=email"> <img src="http://latabak.ru/image/catalog/%D0%97%D0%BD%D0%B0%D1%87%D0%BA%D0%B8/vk.png" /> </a>
</div>

<script type="text/javascript">
    
    function submit() {
        var username = document.getElementById('username').value;
        var password = document.getElementById('password').value;

        $.post('@Url.Action("About", "Home")', { UserName: username, Password: password })
            .done(function (res) {
                alert(res);
            });
    }
</script>